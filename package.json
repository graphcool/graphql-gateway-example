{
  "name": "graphql-server-example",
  "version": "0.0.0",
  "scripts": {
    "dev": "npm-run-all --parallel start playground",
    "start": "nodemon -e ts,graphql -x ts-node -r dotenv/config src/index.ts",
    "playground": "graphql playground",
    "build": "rm -rf dist && graphql prepare && tsc",
    "prisma": "prisma"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "graphql-yoga": "1.3.5",
    "jsonwebtoken": "8.1.1",
    "prisma-binding": "1.5.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.1",
    "dotenv": "4.0.0",
    "graphql-cli": "2.15.5",
    "nodemon": "1.14.11",
    "npm-run-all": "4.1.2",
    "prisma": "1.2.7",
    "ts-node": "5.0.0",
    "typescript": "2.7.1"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true
  }
}
